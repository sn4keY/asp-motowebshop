<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MotoWebShop.MobileApp.Pages.RegisterPage"
             Title="Register">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="EntryUsername" Placeholder="Email" Keyboard="Email"/>
            <Entry x:Name="EntryPassword" Placeholder="Password"/>
            <Button x:Name="ButtonRegister" Text="Register" Clicked="ButtonRegister_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>